<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/register_background"
        android:clickable="true"
        android:orientation="vertical"
        tools:ignore="UselessParent" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:background="#12c1c4" >

            <TextView
                android:id="@+id/sell_release_record_back"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_centerVertical="true"
                android:background="@drawable/text_down_selector"
                android:clickable="true"
                android:drawableLeft="@drawable/ic_action_up_home"
                android:gravity="center"
                android:text="出售发布记录"
                android:textColor="#fff"
                android:textSize="18sp"
                tools:ignore="HardcodedText" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#e6f2f2"
            android:orientation="vertical"
            android:padding="16dp"
            tools:ignore="UselessParent" >

            <TextView
                android:id="@+id/release_date_look"
                style="@style/text_14_42000000"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginBottom="20dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/catch_btn_selector"
                android:gravity="center_vertical"
                android:paddingLeft="16dp"
                android:text="按发布时间查看"
                android:textColor="#8a000000"
                tools:ignore="HardcodedText" />

            <Button
                android:id="@+id/release_date_check"
                style="@style/text_14_42000000"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/catch_btn_selector"
                android:gravity="center_vertical"
                android:paddingLeft="16dp"
                android:text="按审核时间查看"
                android:textColor="#8a000000"
                tools:ignore="HardcodedText" />
        </LinearLayout>
    </LinearLayout>

</FrameLayout>